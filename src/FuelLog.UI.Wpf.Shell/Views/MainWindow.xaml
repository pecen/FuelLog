<Window
  x:Class="FuelLog.UI.Wpf.Shell.Views.MainWindow"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:prism="http://prismlibrary.com/"
  Title="{Binding Title}"
  Width="525"
  Height="350"
  prism:ViewModelLocator.AutoWireViewModel="True">
  <Grid>
    <!--<TabControl prism:RegionManager.RegionName="{Binding TabRegion}">-->
    <!--<ContentControl prism:RegionManager.RegionName="{Binding ContentRegion}">-->
      <Grid>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="300" />
          <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <ContentControl Grid.Column="0" prism:RegionManager.RegionName="{Binding CarListRegion}" />
        <ContentControl Grid.Column="1" prism:RegionManager.RegionName="Detailspage" />
      </Grid>
  </Grid>
</Window>
