<UserControl
  x:Class="FuelLog.UI.Wpf.Module.Views.CarList"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:uc="clr-namespace:FuelLog.UI.Wpf.Module.UserControls"
  xmlns:vm="clr-namespace:FuelLog.UI.Wpf.Module.ViewModels"
  xmlns:Views="clr-namespace:FuelLog.UI.Wpf.Module.Views"
  xmlns:prism="http://prismlibrary.com/"
  prism:ViewModelLocator.AutoWireViewModel="True">
    <UserControl.Resources>
        <!--<DataTemplate x:Key="CarItem" DataType="{x:Type vm:CarItemViewModel}">-->
        <DataTemplate x:Key="CarItem" DataType="{x:Type uc:CarItem}">
            <uc:CarItem />
        </DataTemplate>
    </UserControl.Resources>
    <Grid>
        <ListView ItemTemplate="{StaticResource CarItem}" ItemsSource="{Binding CarItems}">
            <!--<ListView.ItemTemplate>
        <DataTemplate>
          <UserControls:CarItem DataContext="{Binding CarItems}" />
        </DataTemplate>
      </ListView.ItemTemplate>-->
        </ListView>
        <Label
      HorizontalAlignment="Center"
      VerticalAlignment="Center"
      Content="CarList View" />
    </Grid>
</UserControl>
